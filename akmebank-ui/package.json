{
  "name": "akmebank-ui",
  "version": "1.0.0",
  "description": "Akme Bank Web UI",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon ./server.js",
    "ui": "browser-sync start --proxy=localhost:8000 --files='**/*.css, **/*.pug, **/*.js' --ignore=node_modules --reload-delay 10 --no-ui --no-notify",
    "preinstall": "npx npm-force-resolutions"
  },
  "keywords": [
